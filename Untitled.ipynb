   "source": [
    "def power_set(s):\n",
    "    n = len(s)\n",
    "    pow_set_size = 2 ** n\n",
    "    power_set = []\n",
    "\n",
    "    for i in range(pow_set_size):\n",
    "        subset = []\n",
    "        for j in range(n):\n",
    "            if i & (1 << j):\n",
    "                subset.append(s[j])\n",
    "        power_set.append(subset)\n",
    "\n",
    "    return power_set"
   ]
  }
